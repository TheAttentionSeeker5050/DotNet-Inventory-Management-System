@inherits LayoutComponentBase
@inject IBlazorAppBase BlazorAppBase


<TopHeader></TopHeader>
<main class="flex-grow-1 px-3 py-4">
    
    <button type="button" class="btn btn-primary" onclick="history.back()" id="btn-nav-back">
        Go Back
    </button>
        

    <button type="button" class="btn btn-primary" onclick="history.forward()" id="btn-nav-forward">
        Go Forward
    </button>
    @Body
</main>
<Footer></Footer>

<div id="blazor-error-ui">
    An unhandled error has occurred.
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>
